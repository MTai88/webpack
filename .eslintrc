{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "requireConfigFile": false
  },
  "rules": {
    "quotes": "off",
    "no-undef": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "one-var": "off",
    "max-len": "off",
    "no-invalid-this": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off"
  },
  "env": {
     "es6": true,
     "browser": true,
     "node": true
  },
  "extends": ["eslint:recommended", "google"]
}
